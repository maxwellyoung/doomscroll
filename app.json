{
  "expo": {
    "name": "doomscroll",
    "slug": "doomscroll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1771387025319.png",
    "userInterfaceStyle": "dark",
    "scheme": "doomscroll",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#050505"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.maxwellyoung.doomscroll",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-1771387025319.png",
        "backgroundColor": "#050505"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.maxwellyoung.doomscroll"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3d4b84da-adf2-460c-8c99-14af010e06d6"
      }
    },
    "owner": "maxwellyoung"
  }
}
